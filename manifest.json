{
    "manifest_version": 2,

    "name": "Let's stop the smoke",
    "description": "Everyone can quit",
    "options_page": "options.html",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "https://maps.googleapis.com/",
        "geolocation",
        "storage",
        "activeTab",
        "tabs"
    ]
}
